{
  "global": [
    {
      "keys": [
        "Alt",
        "P"
      ],
      "display": "Alt + P",
      "targetId": "goPayments",
      "targetAction": "click",
      "description": "Navigates to payments page by clicking 'goPayments'."
    },
    {
      "keys": [
        "Alt",
        "O"
      ],
      "display": "Alt + O",
      "targetId": "goOrders",
      "targetAction": "click",
      "description": "Navigates to orders page by clicking 'goOrders'."
    }
  ],
  "/orders": [
    {
      "action": "search",
      "keys": [
        "Control",
        "f"
      ],
      "display": "Ctrl + F",
      "targetId": "orderSearchBar",
      "targetAction": "focus",
      "description": "Focuses the order search input field."
    },
    {
      "action": "newOrder",
      "keys": [
        "F2"
      ],
      "display": "Ctrl + N",
      "targetId": "newOrderModalOpenBtn",
      "targetAction": "click",
      "description": "Opens the new order modal by clicking 'newOrderModalOpenBtn'. (Display text may be misleading)"
    }
  ]
}