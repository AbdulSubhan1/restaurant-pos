{
  "global": [
    {
      "action": "go to payments page",
      "keys": [
        "Alt",
        "w"
      ],
      "display": "Alt + W",
      "targetId": "goPayments",
      "targetAction": "click",
      "description": "Navigates to payments page by clicking 'goPayments'."
    },
    {
      "action": "go to Orders page",
      "keys": [
        "Alt",
        "O"
      ],
      "display": "Alt + O",
      "targetId": "goOrders",
      "targetAction": "click",
      "description": "Navigates to orders page by clicking 'goOrders'."
    },
    {
      "action": "Print the visiable page",
      "keys": [
        "Control",
        "p"
      ],
      "display": "Ctrl + P",
      "targetId": "print",
      "targetAction": "click",
      "description": "Prints the current page by clicking 'print'."
    },
    {
      "action": "Download the visiable recipt",
      "keys": [
        "Control",
        "j"
      ],
      "display": "Ctrl + J",
      "targetId": "downloadRecipt",
      "targetAction": "click",
      "description": "donwload the current page by clicking 'print'."
    }
  ],
  "/orders": [
    {
      "action": "search",
      "keys": [
        "Control",
        "f"
      ],
      "display": "Ctrl + F",
      "targetId": "orderSearchBar",
      "targetAction": "focus",
      "description": "Focuses the order search input field."
    },
    {
      "action": "view order",
      "keys": [
        "Alt",
        "v"
      ],
      "display": "Alt + V",
      "targetId": "orderList0",
      "targetAction": "click",
      "description": "Focuses the order search input field."
    },
    {
      "action": "process order",
      "keys": [
        "Alt",
        "p"
      ],
      "display": "Alt + P",
      "targetId": "processOrder",
      "targetAction": "click",
      "description": "Processes the order by clicking 'processOrder'. (Display text may be misleading)"
    },
    {
      "action": "cancel order",
      "keys": [
        "Alt",
        "d"
      ],
      "display": "Alt + D",
      "targetId": "cancelOrder",
      "targetAction": "click",
      "description": "Cancels the order by clicking 'cancelOrder'. (Display text may be misleading)"
    },
    {
      "action": "newOrder",
      "keys": [
        "F2"
      ],
      "display": "Ctrl + N",
      "targetId": "newOrderModalOpenBtn",
      "targetAction": "click",
      "description": "Opens the new order modal by clicking 'newOrderModalOpenBtn'. (Display text may be misleading)"
    }
  ],
  "/payments": [],
  "/kitchen": [],
  "/tables": []
}